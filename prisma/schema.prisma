generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Job {
  id          String   @id @default(uuid())
  title       String
  company     String
  location    String
  url         String   @unique
  datePosted  DateTime @default(now())
  source      String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
